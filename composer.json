{
    "name": "jasekiw/documented-meta",
    "description": "A meta library for laravel",
    "keywords": ["jasekiw", "documented-meta"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.4",
        "illuminate/database": "^5.4"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "LaravelDocumentedMeta\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelDocumentedMeta\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
